{
  "name": "short-stack",
  "version": "2.0.0",
  "description": "Automate the installation and maintenance of your dev stack.",
  "main": "short-stack.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/short-stack/short-stack.git"
  },
  "keywords": [
    "ansible",
    "short-stack",
    "cli"
  ],
  "scripts": {
    "build:webpack": "webpack",
    "build:nexe": "nexe -i ./dist/bundle.js -o ss",
    "prebuild": "npm run build:webpack",
    "build": "npm run build:nexe",
    "build:watch": "concurrently \"npm run build:webpack -- --watch\" \"onchange 'dist/bundle.js' -- npm run build:nexe\""
  },
  "author": "Poetic Systems, LLC <info@poeticsystems.com>",
  "contributors": [
    "Matthew Hager <matthew@poeticsystems.com>",
    "Luke Martin <luke@poeticsystems.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/short-stack/short-stack/issues"
  },
  "homepage": "https://github.com/short-stack/short-stack#readme",
  "bin": {
    "short-stack": "./short-stack"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "js-yaml": "^3.5.5",
    "shelljs": "^0.7.6"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "concurrently": "^3.1.0",
    "nexe": "^1.1.2",
    "onchange": "^3.2.1",
    "webpack": "^2.2.1"
  }
}
